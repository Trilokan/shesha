<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!--Product UOM-->
    <record id="number_uom" model="product.uom">
        <field name="name">Numbers</field>
        <field name="code">NOS</field>
    </record>

    <record id="package_uom" model="product.uom">
        <field name="name">Package</field>
        <field name="code">PACK</field>
    </record>

    <record id="box_uom" model="product.uom">
        <field name="name">Box</field>
        <field name="code">BOX</field>
    </record>

    <record id="set_uom" model="product.uom">
        <field name="name">Set</field>
        <field name="code">SET</field>
    </record>

    <record id="milliliter_uom" model="product.uom">
        <field name="name">Milliliter</field>
        <field name="code">ML</field>
    </record>

    <record id="liter_uom" model="product.uom">
        <field name="name">Liter</field>
        <field name="code">Ltr</field>
    </record>

    <record id="gram_uom" model="product.uom">
        <field name="name">Gram</field>
        <field name="code">GM</field>
    </record>

    <record id="kilogram_uom" model="product.uom">
        <field name="name">Kilogram</field>
        <field name="code">KG</field>
    </record>

    <record id="millimeter_uom" model="product.uom">
        <field name="name">Millimeter</field>
        <field name="code">MM</field>
    </record>

    <record id="centimeter_uom" model="product.uom">
        <field name="name">Centimeter</field>
        <field name="code">CM</field>
    </record>

    <record id="meter_uom" model="product.uom">
        <field name="name">Meter</field>
        <field name="code">Mtr</field>
    </record>


</odoo>